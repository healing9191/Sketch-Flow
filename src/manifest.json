{
  "$schema": "https://raw.githubusercontent.com/BohemianCoding/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "commands": [
    {
      "name": "Slice for iOS",
      "identifier": "sketch-flow.getslice_ios-identifier",
      "shortcut": "ctrl cmd s",
      "script": "./getslice_ios.js",
      "handler": "sliceIOS"
    },
    {
      "name": "Slice for Android",
      "identifier": "sketch-flow.getslice_android-identifier",
      "shortcut": "ctrl cmd a",
      "script": "./getslice_android.js",
      "handler": "sliceAndroid"
    },
    {
      "name": "Text Code for iOS",
      "identifier": "sketch-flow.textCode_ios-identifier",
      "shortcut": "ctrl cmd w",
      "script": "./getslice_ios.js",
      "handler": "copyTextCode"
    },
    {
      "name": "Text Code for Android",
      "identifier": "sketch-flow.textCode_android-identifier",
      "shortcut": "ctrl cmd e",
      "script": "./getslice_android.js",
      "handler": "copyTextCode"
    },
    {
      "name": "Reset Project Settings",
      "identifier": "sketch-flow.getslice_reset-identifier",
      "shortcut": "ctrl cmd r",
      "script": "./utils.js",
      "handler": "resetProjectSettings"
    },
    {
      "name": "Create a icon template",
      "identifier": "sketch-flow.create_icon_template-identifier",
      "shortcut": "ctrl cmd -",
      "script": "./iconTemplate.js",
      "handler": "importArtBoard"
    }
  ],
  "menu": {
    "title": "Sketch Flow",
    "items": [
      "sketch-flow.getslice_ios-identifier",
      "sketch-flow.getslice_android-identifier",
      "-",
      "sketch-flow.textCode_ios-identifier",
      "sketch-flow.textCode_android-identifier",
      "-",
      "sketch-flow.getslice_reset-identifier",
      "-",
      "sketch-flow.create_icon_template-identifier"
    ]
  },
  "identifier": "me.hite.sketch-flow",
  "homepage": "https://github.com/hite/Sketch-Flow",
  "version": "2.2.4",
  "description" : "Make icon slicing simple for app developers!",
  "authorEmail" : "iamhite@gmail.com",
  "name" : "Sketch Flow"
}