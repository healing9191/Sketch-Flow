{
  "extends": "standard",
  "rules": {
    "space-before-function-paren": "off",
    "one-var": "off",
    "eol-last": "off",
    "indent": ["error", 4]
  },
  "globals": {
    "NSFileManager": "readonly",
    "nil": "readonly",
    "context": "readonly",
    "log": "readonly",
    "NSPasteboard": "readonly",
    "NSString": "readonly",
    "COSAlertWindow": "readonly",
    "NSTextField": "readonly",
    "NSView": "readonly",
    "NSTextView": "readonly",
    "NSDocumentController": "readonly",
    "MSApplicationMetadata": "readonly",
    "NSMakeRect": "readonly"
  }
}